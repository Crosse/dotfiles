{
  "C_Cpp.default.cStandard": "c11",
  "C_Cpp.default.cppStandard": "c++11",
  "C_Cpp.default.intelliSenseMode": "clang-x64",
  "bracketPairColorizer.forceIterationColorCycle": true,
  "bracketPairColorizer.forceUniqueOpeningColor": true,
  "bracketPairColorizer.showBracketsInGutter": true,
  "bracketPairColorizer.showHorizontalScopeLine": false,
  "bracketPairColorizer.showVerticalScopeLine": false,
  "debug.console.fontFamily": "\"SauceCodePro Nerd Font\", Menlo, Monaco, 'Courier New', monospace",
  "editor.cursorBlinking": "smooth",
  "editor.fontFamily": "'SauceCodePro Nerd Font', 'FuraCode Nerd Font Mono', Menlo, Monaco, 'Courier New', monospace",
  "editor.fontSize": 13,
  "editor.fontWeight": "600",
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "editor.hover.delay": 500,
  "extensions.closeExtensionDetailsOnViewChange": true,
  "files.autoGuessEncoding": true,
  "files.exclude": {
    "**/target": true,
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "go.lintTool": "golangci-lint",
  "go.useLanguageServer": true,
  "python.formatting.provider": "black",
  "remote.SSH.defaultExtensions": [
    "christian-kohler.path-intellisense",
    "davidanson.vscode-markdownlint",
    "eamodio.gitlens",
    "editorconfig.editorconfig",
    "ms-python.python",
    "ms-vscode.cpptools",
    "ms-vscode.go",
    "rebornix.ruby",
    "redhat.vscode-yaml",
    "rust-lang.rust",
    "streetsidesoftware.code-spell-checker",
    "timonwong.shellcheck"
  ],
  "rust.clippy_preference": "on",
  "search.smartCase": true,
  "search.useGlobalIgnoreFiles": true,
  "terminal.integrated.fontFamily": "'SauceCodePro Nerd Font', Menlo, Monaco, 'Courier New', monospace",
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": [
        "<C-j>"
      ],
      "commands": [
        ":tabn"
      ]
    },
    {
      "before": [
        "<C-k>"
      ],
      "commands": [
        ":tabp"
      ]
    }
  ],
  "workbench.fontAliasing": "auto",
  "workbench.startupEditor": "newUntitledFile",
  "zenMode.hideLineNumbers": false,
  "zenMode.hideStatusBar": false,
  "zenMode.hideTabs": false,
  "C_Cpp.intelliSenseEngineFallback": "Enabled",
  "python.formatting.blackPath": "${env:HOME}/.local/bin/black",
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "vsicons.dontShowNewVersionMessage": true,
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "java.semanticHighlighting.enabled": true,
  "remote.SSH.configFile": "/Users/seth/.config/ssh/vscode",
  "cSpell.allowCompoundWords": true,
  "cSpell.enabled": false,
  "java.configuration.runtimes": [
    {
      "name": "JavaSE-17",
      "path": "/Users/seth/Library/Java/JavaVirtualMachines/amazon-corretto-17-x64.jdk/Contents/Home"
    }
  ],
  "go.toolsManagement.autoUpdate": true,
  "codeQL.telemetry.enableTelemetry": true,
  "editor.tabSize": 8,
  "editor.renderWhitespace": "none",
  "editor.scrollbar.vertical": "visible",
  "editor.smoothScrolling": true,
  "editor.cursorSurroundingLines": 3,
  "editor.fontLigatures": true,
  "editor.formatOnSaveMode": "modificationsIfAvailable",
  "diffEditor.codeLens": true,
  "go.enableCodeLens": {},
  "editor.quickSuggestionsDelay": 100,
  "editor.suggest.preview": true,
  "files.insertFinalNewline": true,
  "workbench.colorTheme": "Solarized Light+",
  "workbench.tree.indent": 20,
  "workbench.view.alwaysShowHeaderActions": true,
  "workbench.tree.renderIndentGuides": "always",
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.untitled.hint": "hidden",
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.patterns": {
    "*.ts": "${capture}.js",
    "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
    "*.jsx": "${capture}.js",
    "*.tsx": "${capture}.ts",
    "tsconfig.json": "tsconfig.*.json",
    "package.json": "package-lock.json, yarn.lock"
  },
  "explorer.sortOrderLexicographicOptions": "unicode",
  "search.exclude": {
    "**/vendor": true
  },
  "search.seedWithNearestWord": true,
  "terminal.external.osxExec": "iTerm.app",
  "terminal.integrated.bellDuration": 100,
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.enableBell": true,
  "terminal.integrated.ignoreProcessNames": [
    "starship",
    "oh-my-posh",
    "bash",
    "zsh",
    "tmux"
  ],
  "terminal.integrated.macOptionIsMeta": true,
  "terminal.integrated.rightClickBehavior": "nothing",
  "terminal.integrated.shellIntegration.enabled": true,
  "problems.showCurrentInStatus": true,
  "comments.useRelativeTime": false,
  "editorconfig.generateAuto": false,
  "git.enableCommitSigning": true,
  "github.gitProtocol": "ssh",
  "remote.SSH.remoteServerListenOnSocket": true,
  "vim.startofline": false,
  "vim.statusBarColorControl": true,
  "workbench.colorCustomizations": {
    "statusBar.background": "#005f5f",
    "statusBar.noFolderBackground": "#005f5f",
    "statusBar.debuggingBackground": "#005f5f"
  },
  "cSpell.userWords": [
    "behaviour",
    "Bukkit",
    "Modrinth",
    "Mojang"
  ],
  "rust-analyzer.inlayHints.chainingHints.enable": false,
  "rust-analyzer.inlayHints.typeHints.enable": false,
  "rust-analyzer.inlayHints.parameterHints.enable": false,
}
